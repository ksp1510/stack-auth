{
  "name": "stack-auth",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter './apps/*' dev",
    "build": "pnpm -r --filter './apps/*' build",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "devDependencies": {
    "@angular/cli": "^21.1.0"
  },
  "pnpm": {
    "overrides": {
      "lucide-react": "0.562.0"
    }
  }
}
